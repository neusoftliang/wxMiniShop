<!--componets/w-swiper/w-swiper.wxml-->
<!-- 轮播图 -->
<swiper class="l-swiper"
        autoplay
        indicator-dots
        indicator-active-color="#ff5777"
        circular
        interval="3000">
  <block wx:for="{{images}}" wx:key="index">
    <swiper-item>
      <image class="swiper-image" src="{{item}}"></image>
    </swiper-item>
  </block>
</swiper>
